{
  "coc.preferences.formatOnSaveFiletypes": [
    "Markdown",
    "css",
    "jsx",
    "javascript",
    "javascript.jsx",
    "javascriptreact",
    "tsx",
    "typescript",
    "typescript.tsx",
    "typescriptreact"
  ],
  "coc.preferences.hoverTarget":"float",
  "suggest.floatEnable": true,
  "suggest.echodocSupport": true,
  "diagnostic.virtualText": false,
  "diagnostic.virtualTextPrefix": "***",
  // eslint
  "eslint.filetypes": [
    "javascript",
    "javascript.jsx",
    "typescript",
    "typescript.jsx"
  ]
}
